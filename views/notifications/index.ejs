<%- include('../partials/header') %>
<h1>Notifications</h1>

<ul>
    <% allNotifications.forEach(function(notification) { %>
    <li>
        <a href="/campgrounds/<%= notification.campgroundSlug %>">
            <%= notification.username %> created a new campground %>
        </a>
    </li>
    <% }); %>
</ul>
